{
  "name": "inflection-table",
  "version": "REPLACE",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/inflection-table.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/inflection-table.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/inflection-table.min.js"
  },
  "main": "dist/inflection-table.umd.js",
  "module": "dist/inflection-tablet.esm.js",
  "unpkg": "dist/inflection-table.min.js",
  "dependencies": {
    "jquery": "^3.5.1",
    "core-js": "~3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "acorn": "^8.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.11"
  }
}
